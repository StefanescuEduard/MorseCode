#include <string.h>

char* get_word(char *line, char *delimeter, int iterations);

char* get_morse(char* line)
{
	return get_word(line, ":", 1);
}

char* get_word(char *line, char *delimeter, int iterations)
{
	int current_index = 0;
	char *word = NULL;
	
	while (current_index < iterations)
	{
		word = strtok(line, delimeter);
		word = strtok(line, NULL);
		current_index++;
	}

	return word;
}